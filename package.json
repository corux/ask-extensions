    "i18next-sprintf-postprocessor": "^0.2.2",
    "luxon": "^1.15.0"
  },
  "devDependencies": {
    "@types/i18next-sprintf-postprocessor": "0.0.29",
    "@types/jest": "24.0.13",
    "@types/luxon": "1.15.1",
    "@types/node": "10.14.8",
    "jest": "24.8.0",
    "npm-run-all": "4.1.5",
    "ts-jest": "24.0.2",
    "ts-node": "8.2.0",
    "tslint": "5.16.0",
    "typescript": "3.5.1"
